BloxFruit.Active = true
BloxFruit.Draggable = true

TextLabel.Parent = BloxFruit
TextLabel.BackgroundColor3 = Color3.fromRGB(0, 3, 213)
TextLabel.Size = UDim2.new(0, 366, 0, 20)
TextLabel.Font = Enum.Font.GothamBlack
TextLabel.Text = "BloxFruit"
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextSize = 14.000

