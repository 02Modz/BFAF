end)

Close.Name = "Close"
Close.Parent = BloxFruit
Close.BackgroundColor3 = Color3.fromRGB(255, 0, 4)
Close.BorderColor3 = Color3.fromRGB(0, 0, 0)
Close.Position = UDim2.new(0.898907125, 0, 0, 0)
