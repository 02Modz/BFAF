
local ScreenGui = Instance.new("ScreenGui")
local BloxFruit = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local BloxFruitAF = Instance.new("TextButton")
local MainStuff = Instance.new("TextButton")
local Close = Instance.new("TextButton")
local openBloxFruit = Instance.new("Frame")
local open = Instance.new("TextButton")
--Properties:
ScreenGui.Parent = game.CoreGui

BloxFruit.Name = "BloxFruit"
BloxFruit.Parent = ScreenGui
BloxFruit.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
